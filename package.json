  "main": "server.js",
  "scripts": {
    "test": "npm run seed && NODE_ENV=test mocha --compilers js:babel-core/register server/test/**/*.spec.js",
    "start": "webpack && babel-node server.js --presets es2015,stage-2",
    "seed": "NODE_ENV=test node server/seeders/seeders",
    "coverage": "npm run seed && ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha server/test/**/*.spec.js -- --require babel-register",
